import random
amount, number = input().split('d')
result = 0
summa = 0
for i in range(int(amount)):
  result = random.randint(1, int(number))
  i += 1
  summa += result
print (summa)

#как вариант
result = []
for i in range(int(amount)):
  result.append(random.randint(1, int(number)))
summ = sum(result)
